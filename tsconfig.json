{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "types": ["jest"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["pwa/**/*.ts", "pwa/**/*.tsx", "gas/**/*.js"]
}
